	<sql id="Authority_Column_List">
		AUTHORITY_ID,
		AUTHORITY_NAME,AUTHORITY_NO,AUTHORITY_DESC,AUTHORITY_ICON,CREATE_TIME,LAST_MODIFY_TIME,DELETE_FLAG,MEMO
	</sql>
	<select id="findAllAuthority" resultType="Authority" resultMap="resultAuthList">
		SELECT * FROM LEGOO_AUTH;
	</select>
	<select id="count" parameterType="Authority" resultType="int">
		select count(1) from LEGOO_AUTH where 1=1
		<if test="authorityId != null">
			AND AUTHORITY_ID= #{authorityId}
		</if>
		<if test="authorityName != null">
			AND AUTHORITY_NAME= #{authorityName}
		</if>
		<if test="authorityNo != null">
			AND AUTHORITY_NO= #{authorityNo}
		</if>
		<if test="authorityIcon != null">
			AND AUTHORITY_ICON= #{authorityIcon}
		</if>
		<if test="createTime != null">
			AND CREATE_TIME= #{createTime}
		</if>
		<if test="lastModifyTime != null">
			AND LAST_MODIFY_TIME= #{lastModifyTime}
		</if>
		<if test="authorityDesc != null">
			AND AUTHORITY_DESC= #{authorityDesc}
		</if>
		<if test="deleteFlag != null">
			AND DELETE_FLAG= #{deleteFlag}
		</if>
		<if test="memo != null">
			AND MEMO= #{memo}
		</if>
	</select>
	<insert id="insert" parameterType="Authority">
		insert into LEGOO_AUTH
		<trim prefix="(" suffix=")" suffixOverrides=",">
			AUTHORITY_ID,
			<if test="authorityName != null">
				AUTHORITY_NAME,
			</if>
			<if test="authorityNo != null">
				AUTHORITY_NO,
			</if>
			<if test="createTime != null">
				CREATE_TIME,
			</if>
			<if test="lastModifyTime != null">
				LAST_MODIFY_TIME,
			</if>
			<if test="authorityDesc != null">
				AUTHORITY_DESC,
			</if>
			<if test="authorityIcon != null">
				AUTHORITY_Icon,
			</if>
			<if test="deleteFlag != null">
				DELETE_FLAG,
			</if>
			<if test="memo != null">
				MEMO,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			UUID_SHORT(),
			<if test="authorityName != null">
				#{authorityName},
			</if>
			<if test="authorityNo != null">
				#{authorityNo},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="lastModifyTime != null">
				#{lastModifyTime},
			</if>
			<if test="authorityDesc != null">
				#{authorityDesc},
			</if>
			<if test="authorityIcon != null">
				#{authorityIcon},
			</if>
			<if test="deleteFlag != null">
				#{deleteFlag},
			</if>
			<if test="memo != null">
				#{memo},
			</if>
		</trim>
	</insert>
	<delete id="deleteById" parameterType="Authority">
		delete from LEGOO_AUTH
		where AUTHORITY_ID = #{authorityId}
	</delete>
	<delete id="deleteByMap" parameterType="Authority">
		delete from LEGOO_AUTH
		<where>
			<if test="authorityId != null">
				AND AUTHORITY_ID= #{authorityId}
			</if>
			<if test="authorityName != null">
				AND AUTHORITY_NAME= #{authorityName}
			</if>
			<if test="authorityNo != null">
				AND AUTHORITY_NO= #{authorityNo}
			</if>
			<if test="createTime != null">
				AND CREATE_TIME= #{createTime}
			</if>
			<if test="lastModifyTime != null">
				AND LAST_MODIFY_TIME= #{lastModifyTime}
			</if>
			<if test="authorityDesc != null">
				AND AUTHORITY_DESC= #{authorityDesc}
			</if>
			<if test="authorityIcon != null">
				AND AUTHORITY_ICON= #{authorityIcon}
			</if>
			<if test="deleteFlag != null">
				AND DELETE_FLAG= #{deleteFlag}
			</if>
			<if test="memo != null">
				AND MEMO= #{memo}
			</if>
		</where>
	</delete>
	<update id="update" parameterType="Role">
		update LEGOO_AUTH
		<set>
			<if test="authorityName != null">
				#{authorityName},
			</if>
			<if test="authorityNo != null">
				#{authorityNo},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="lastModifyTime != null">
				#{lastModifyTime},
			</if>
			<if test="authorityDesc != null">
				#{authorityDesc},
			</if>
			<if test="authorityIcon != null">
				#{authorityIcon},
			</if>
			<if test="deleteFlag != null">
				#{deleteFlag},
			</if>
			<if test="memo != null">
				#{memo},
			</if>
		</set>
		where AUTHORITY_ID = #{authorityId}
	</update>

	<select id="findAuthorityById" resultType="Authority" resultMap="resultAuthList">
		select
		<include refid="Authority_Column_List" />
		from LEGOO_AUTH where AUTHORITY_ID = #{authorityId}
	</select>
	<select id="selectByProperties" parameterType="Authority"
		resultMap="resultAuthList">
		select
		<include refid="Authority_Column_List" />
		from LEGOO_AUTH where 1=1
		<if test="authorityId != null">
			AND AUTHORITY_ID= #{authorityId}
		</if>
		<if test="authorityName != null">
			AND AUTHORITY_NAME= #{authorityName}
		</if>
		<if test="authorityNo != null">
