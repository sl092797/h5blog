<mapper namespace="com.victor.h5blog.dao.AuthorityDao">
	<resultMap id="resultAuthList" type="Authority">
		<id column="AUTHORITY_ID" property="authorityId" />
		<result column="AUTHORITY_NAME" property="authorityName" />
		<result column="AUTHORITY_NO" property="authorityNo" />
		<result column="AUTHORITY_DESC" property="authorityDesc" />
		<result column="AUTHORITY_ICON" property="authorityIcon" />
		<result column="CREATE_TIME" property="createTime" />
